{
  "name": "collin-newman-service",
  "version": "1.0.0",
  "description": "handle the display of reviews for a selected trip-advisor location",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --testTimeout=10000 --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --verbose --coverage --colors",
    "scrape": "node database/seeding/scrapeimages.js",
    "seed": "node ./database/seeding/seed.js",
    "start": "nodemon ./server/server.js",
    "react:dev": "webpack --watch",
    "build": "webpack --mode=production",
    "webpack": "webpack --profile --json > stats.json",
    "lint": "./node_modules/.bin/eslint . --ignore-path ./.eslintignore",
    "csv:dest": "node database/seeding/createCSV.js",
    "csv:rev": "node database/seeding/createReviewCSV.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slack-Reactor/collin-newman-service.git"
  },
  "keywords": [
    "reviews"
  ],
  "author": "collin-newman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Slack-Reactor/collin-newman-service/issues"
  },
  "homepage": "https://github.com/Slack-Reactor/collin-newman-service#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^5.3.1",
    "fast-csv": "^4.3.6",
    "fontsource-roboto": "^4.0.0",
    "jest": "^25.0.0",
    "material-ui-dropzone": "^3.5.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.11",
    "newrelic": "^7.1.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-material-ui-carousel": "^2.1.2",
    "react-router-dom": "^5.2.0",
    "wait-port": "^0.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
